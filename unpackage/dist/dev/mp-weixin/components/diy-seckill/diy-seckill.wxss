.diy-seckill .marketing-wrap {
  background-color: #ffffff;
  border-radius: 10rpx;
  overflow: hidden;
  box-sizing: border-box;
  position: relative;
}
.diy-seckill .marketing-wrap > image {
  width: 100%;
  position: absolute;
  top: 0;
  z-index: 1;
}
.diy-seckill .marketing-wrap .marketimg-box {
  padding: 30rpx 0;
  position: relative;
  z-index: 2;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title {
  display: flex;
  height: 32rpx;
  line-height: 32rpx;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-left {
  display: flex;
  align-items: center;
  line-height: 32rpx;
  margin-right: 18rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-left image {
  width: 126rpx;
  margin-right: 20rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-left text {
  color: #909399;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .seckill-title-time {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .seckill-title-time .seckill-title-name {
  font-size: 28rpx;
  margin-right: 7rpx;
  line-height: 1;
  color: #777777;
  font-size: 28rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .seckill-title-time .symbol {
  padding-left: 6rpx;
  padding-right: 6rpx;
  height: 28rpx;
  line-height: 24rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .seckill-title-time .seckill-H {
  width: 30rpx;
  height: 30rpx;
  background-color: #383838;
  color: #ffffff;
  font-size: 24rpx;
  text-align: center;
  line-height: 30rpx;
  border-radius: 3rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .seckill-title-time .seckill-I {
  width: 28rpx;
  height: 28rpx;
  background-color: #383838;
  color: #ffffff;
  font-size: 20rpx;
  text-align: center;
  line-height: 28rpx;
  border-radius: 5rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .seckill-title-time .seckill-S {
  width: 28rpx;
  height: 28rpx;
  background-color: #ff4544;
  color: #ffffff;
  font-size: 20rpx;
  text-align: center;
  line-height: 28rpx;
  border-radius: 5rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-right {
  display: flex;
  align-items: center;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-right text {
  font-size: 24rpx;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-right.red text {
  background: linear-gradient(-55deg, #ff5151 0%, #ff7b91 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-right.blue text {
  background: linear-gradient(-55deg, #0ecfd3 0%, #12d0ae 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-right.yellow text {
  background: linear-gradient(-55deg, #feb632 0%, #fe6232 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.diy-seckill .marketing-wrap .marketimg-box .marketimg-box-title .marketimg-box-title-right.violet text {
  background: linear-gradient(-55deg, #627bfd 0%, #8662fd 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
