.kf-line {
  background-color: red;
}
.kf-component .vkefu {
  z-index: 11;
  position: fixed;
  width: 80rpx;
  height: 80rpx;
  border-radius: 80rpx;
  color: #fff;
  background: rgba(0, 0, 0, 0.5);
  bottom: 250rpx;
  right: 20rpx;
  white-space: nowrap;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  opacity: 0;
}
.kf-component .kf-style {
  z-index: 10;
  position: fixed;
  width: 80rpx;
  height: 80rpx;
  border-radius: 80rpx;
  color: #fff;
  bottom: 250rpx;
  right: 20rpx;
}
.kf-component .kf-style image {
  width: 100%;
  height: 100%;
}

