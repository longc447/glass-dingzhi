@charset "UTF-8";
/**
 * 你可以通过修改这些变量来定制自己的插件主题，实现自定义主题功能
 * 建议使用scss预处理，并在插件代码中直接使用这些变量（无需 import 这个文件），方便用户通过搭积木的方式开发整体风格一致的App
 */
page {
  background-color: #fff;
}
.grow-explain {
  padding: 30rpx;
  margin-top: 30rpx;
}
.grow-explain .explain-title {
  display: flex;
  align-items: center;
  justify-content: center;
  line-height: 1;
  margin-bottom: 40rpx;
}
.grow-explain .explain-title image {
  margin: 0 20rpx;
  width: 54rpx;
  height: 18rpx;
}
.grow-explain .explain-tr {
  display: flex;
}
.grow-explain .explain-th {
  padding: 10rpx 30rpx;
  flex: 1;
  background-color: #f6f1e4;
}
.grow-explain .explain-th ~ .explain-th {
  border-left: 4rpx solid #fff;
}
.grow-explain .explain-td {
  padding: 10rpx 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  flex: 1;
  background-color: #fcfbf7;
}
.grow-explain .explain-td ~ .explain-td {
  border-left: 4rpx solid #fff;
}
.grow-value,
.acquisition-grow {
  padding: 0 30rpx 30rpx;
}
.grow-value .title,
.acquisition-grow .title {
  display: flex;
  align-items: center;
}
.grow-value .title image,
.acquisition-grow .title image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.grow-value .content,
.acquisition-grow .content {
  font-size: 24rpx;
  margin-left: 40rpx;
}
.grow-value .content text,
.acquisition-grow .content text {
  display: block;
}

